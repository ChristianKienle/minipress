{
  "name": "@minipress/main",
  "version": "0.1.1-alpha.4",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://christiankienle.github.io/minipress/",
  "scripts": {
    "publish:npm": "lerna publish prerelease",
    "gh-pages": "node _support/gh-pages",
    "bs": "lerna bootstrap",
    "clean": "lerna clean -y",
    "docs": "cd docs && npm run dev",
    "docs:debug": "cd docs && npm run dev:debug",
    "docs:gen": "cd docs && npm run gen",
    "docs:serve": "cd docs && npm run serve",
    "docs:gen:debug": "cd docs && npm run gen:debug",
    "helloworld": "cd _examples/hello-world && npm run dev",
    "helloworld:serve": "cd _examples/hello-world && npm run serve",
    "helloworld:debug": "cd _examples/hello-world && npm run dev:debug",
    "helloworld:gen": "cd _examples/hello-world && npm run gen",
    "helloworld:gen:debug": "cd _examples/hello-world && npm run gen:debug",
    "features": "cd _examples/features && npm run dev -- --port 4001",
    "features:debug": "cd _examples/features && npm run dev:debug",
    "features:gen": "cd _examples/features && npm run gen",
    "features:serve": "cd _examples/features && npm run serve",
    "features:gen:debug": "cd _examples/features && npm run gen:debug"
  },
  "devDependencies": {
    "lerna": "^3.16.4"
  }
}
