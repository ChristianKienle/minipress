{
  "compilerOptions": {
    "esModuleInterop": true,
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "target": "esnext",
    "module": "commonjs",
    "resolveJsonModule": true,
    "strict": true,
    "moduleResolution": "node",
    "noImplicitAny": false,
  },
  "include": ["**/*.js"]
}